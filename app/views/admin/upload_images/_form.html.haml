= semantic_form_for :upload_images, builder: ActiveAdmin::FormBuilder, url: { action: :grab } do |f|
  =f.inputs do
    =f.input :url, label: false, placeholder: 'Paste your URL'
    -#=f.input :authenticity_token, type: :hidden, name: :authenticity_token, value: form_authenticity_token
    =f.action :submit, label: 'Get Images'